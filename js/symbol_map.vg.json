{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "New york map of Airbnbs",
  "projection": {
    "type": "albersUsa"
    },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/louisdecastella/3179/main/data/new_york_map2.topojson",
        "format": {"type": "topojson", "feature": "ne_10m_admin_2_counties"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
      "url":
      "https://raw.githubusercontent.com/louisdecastella/3179/main/data/Airbnb_Open_Data.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
      "longitude": {
        "field": "longditude",
         "type":"quantitative"
        },
      "latitude": {
        "field": "latitude", 
        "type":"quantitative"
      },
      "color": {
      "field": "Construction year",
      "type": "quantitative",
      "title": "Construction Year",
      "scale": {"scheme": "reds"}
      }
      }
      }
      ]
      }
